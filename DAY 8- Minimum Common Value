2540. Minimum Common Value
//Using Two pointers
class Solution {
    public int getCommon(int[] nums1, int[] nums2) {
     int n=nums1.length;
     int m=nums2.length;
     int val=0;
     int p1=0,p2=0;
     while(p1<n && p2<m){
       if(nums1[p1]<nums2[p2]){
        p1++;
       }
       else if(nums1[p1]>nums2[p2]){
        p2++;
       }
       else{
         return nums1[p1];
       }
     }
       return -1;
    }
}
